{
  "compilerOptions": {
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      // "@main/*": ["main/*"],
      // "@renderer/*": ["renderer/*"],
      // "@lyric/*": ["renderer-lyric/*"],
      // "@static/*": ["static/*"],
      "*": [
        "src/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      // "@common/*": ["common/*"],
      "@MVC/*": [
        "src/MVC/*"
      ]
    }
  },
  "vueCompilerOptions": {
    "plugins": [
      "@vue/language-plugin-pug"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ]
}